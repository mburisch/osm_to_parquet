{
	"name": "Ubuntu",
	"image": "mcr.microsoft.com/devcontainers/base:jammy",
	"features": {
		"ghcr.io/devcontainers/features/common-utils:2": {
			"upgradePackages": true,
			"username": "vscode",
			"userUid": "1001",
			"userGid": "1001"
		},
		"ghcr.io/devcontainers/features/git:1": {
			"version": "latest",
			"ppa": "false"
		},
		"ghcr.io/devcontainers/features/kubectl-helm-minikube:1": {
			"version": "latest",
			"helm": "latest",
			"minikube": "none"
		},
		"ghcr.io/devcontainers/features/python:1": {
			"installTools": true,
			"version": "3.13"
		},
		"ghcr.io/devcontainers/features/docker-outside-of-docker:1": {
			"enableNonRootDocker": "true",
			"moby": "true"
		},
		"ghcr.io/devcontainers/features/rust:1": {
			"version": "latest",
			"profile": "default"
		},
		"ghcr.io/va-h/devcontainers-features/uv:1": {},
		"ghcr.io/devcontainers-extra/features/protoc:1": {},
		"ghcr.io/devcontainers-extra/features/apt-get-packages:1": {
			"packages": "curl, gnupg, jq, less, iputils-ping, dnsutils, htop, openjdk-21-jre, openjdk-21-jdk"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-kubernetes-tools.vscode-kubernetes-tools",
				"redhat.vscode-yaml",
				"visualstudioexptteam.vscodeintellicode",
				"ms-toolsai.jupyter",
				"ms-toolsai.vscode-jupyter-cell-tags",
				"ms-toolsai.jupyter-renderers",
				"zxh404.vscode-proto3"
			]
		}
	},
	"remoteUser": "vscode",
	"mounts": [
		"source=data,target=/workspaces/data,type=volume",
		"source=venvs,target=/workspaces/venvs,type=volume",
		"source=build,target=/workspaces/build,type=volume"
	],
	"containerEnv": {
		"UV_CACHE_DIR": "/workspaces/build/uv/",
		"UV_LINK_MODE": "copy",
		"CARGO_TARGET_DIR": "/workspaces/build/projects/osm_to_parquet/"
	}
}